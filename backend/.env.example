# AgentHub Backend Environment Variables
# Copy this file to .env and fill in your values

# Server
PORT=5000
NODE_ENV=development

# Database (MySQL)
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_mysql_password
DB_NAME=agenthub

# JWT Secrets (use strong random strings in production)
JWT_ACCESS_SECRET=agenthub_access_secret_change_in_production_2025
JWT_REFRESH_SECRET=agenthub_refresh_secret_change_in_production_2025
JWT_ACCESS_EXPIRES=15m
JWT_REFRESH_EXPIRES=7d

# CORS
FRONTEND_URL=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
